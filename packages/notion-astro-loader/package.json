{
  "name": "notion-astro-loader",
  "workspaces": [
    "packages/notion-astro-loader"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schemas": {
      "import": "./dist/schemas/index.js",
      "types": "./dist/schemas/index.d.ts"
    },
    "./schemas/properties": {
      "import": "./dist/schemas/properties.js",
      "types": "./dist/schemas/properties.d.ts"
    }
  },
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "astro": "^4.14.0"
  },
  "dependencies": {
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@notionhq/client": "^2.2.0",
    "notion-rehype-k": "^1.1.6",
    "rehype-katex": "^7.0.0",
    "rehype-shift-heading": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "slug": "^9.1.0",
    "unified": "^11.0.0"
  },
  "devDependencies": {
    "@types/slug": "^5.0.9",
    "astro": "^4.14.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
